{
  "name": "vscode-theme-beach-condo",
  "displayName": "Beach Condo Color Themes",
  "description": "A set of custom color themes for VSCode",
  "version": "0.0.1",
  "publisher": "mimshwright",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "watch": "node scripts/build.js && echo 'Watching for changes...' && nodemon --watch src --exec 'node scripts/build.js'"
  },
  "contributes": {
    "themes": [
      {
        "label": "Beach Condo - Belize City - Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/beach-condo-belize-city-dark.json"
      },
      {
        "label": "Beach Condo - Belize City - Light",
        "uiTheme": "vs",
        "path": "./themes/beach-condo-belize-city-light.json"
      },
      {
        "label": "Beach Condo - Barcelona - Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/beach-condo-barcelona-dark.json"
      },
      {
        "label": "Beach Condo - Barcelona - Light",
        "uiTheme": "vs",
        "path": "./themes/beach-condo-barcelona-light.json"
      },
      {
        "label": "Beach Condo - Bangkok - Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/beach-condo-bangkok-dark.json"
      },
      {
        "label": "Beach Condo - Bangkok - Light",
        "uiTheme": "vs",
        "path": "./themes/beach-condo-bangkok-light.json"
      },
      {
        "label": "Beach Condo - Brisbane - Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/beach-condo-brisbane-dark.json"
      },
      {
        "label": "Beach Condo - Brisbane - Light",
        "uiTheme": "vs",
        "path": "./themes/beach-condo-brisbane-light.json"
      }
    ]
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
